{
  "name": "hhrtf_application",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "watch": "NODE_OPTIONS=--openssl-legacy-provider webpack -w",
    "start:noReload": "concurrently -k -n \" WATCH ,WEBPACK\" -c \"yellow.bold,blue.bold\" \"npm run browsersyncNoReload\" \"npm run watch\"",
    "start": "NODE_ENV=development concurrently -k -n \" WATCH ,WEBPACK\" -c \"yellow.bold,blue.bold\" \"npm run browsersync\" \"npm run watch\"",
    "browsersync": "node bs.js",
    "browsersyncNoReload": "browser-sync start --server --startPath ./public --port 4000",
    "start:ie": "cross-env NODE_ENV=qa npm run start",
    "test": "jest --passWithNoTests",
    "precommit": "npm run lint",
    "lint": "eslint src --ext .js --ext .jsx --fix",
    "build": "npm run stage && npm run browsersync",
    "stage": "cross-env NODE_ENV=production webpack -p --progress",
    "production": "cross-env NODE_ENV=production PROD_URL=production webpack -p --progress",
    "bhelp": "webpack --help"
  },
  "keywords": [
    "FireNet Designs"
  ],
  "author": "Justin Schellenberg",
  "license": "proprietary",
  "devDependencies": {
    "@babel/core": "~7.9.6",
    "@types/jest": "~24.9.1",
    "autoprefixer": "~9.7.6",
    "babel-loader": "~8.1.0",
    "babel-preset-core3": "^7.0.1",
    "browser-sync": "^2.26.7",
    "chalk": "^3.0.0",
    "concurrently": "~4.1.2",
    "connect-history-api-fallback": "~1.6.0",
    "cross-env": "~5.2.1",
    "css-loader": "~1.0.1",
    "dotenv-webpack": "~1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-core3": "^6.1.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "exports-loader": "~0.6.4",
    "file-loader": "^3.0.1",
    "glob": "~7.1.4",
    "imports-loader": "~0.7.1",
    "jest": "~24.9.0",
    "mini-css-extract-plugin": "~0.5.0",
    "sass": "^1.56.1",
    "optimize-css-assets-webpack-plugin": "~5.0.1",
    "postcss-loader": "~3.0.0",
    "prettier": "~2.0.5",
    "sass-loader": "~7.3.1",
    "shx": "^0.3.2",
    "terser-webpack-plugin": "~1.4.3",
    "wait-on": "~3.3.0",
    "webpack": "~4.43.0",
    "webpack-cli": "~3.3.11"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "classnames": "^2.2.6",
    "core-js": "3.3.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
